{
    "compilerOptions": {
      "target": "ES6",
      "outDir": "./dist",
      "module": "CommonJS",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "declaration": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "skipLibCheck": true,
      "baseUrl": "."
    },
    "include": ["**/*", "./.env"]
  }
  